    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@prisma/client": "^5.14.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcrypt": "^5.1.1",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "ioredis": "^5.4.1",
    "ip-cidr": "^4.0.2",
